<tables>
<table>
<name>vtiger_entitylog</name>
<sql><![CDATA[CREATE TABLE `vtiger_entitylog` (
  `entitylogid` int(11) NOT NULL,
  `entitylogname` varchar(255) DEFAULT NULL,
  `relatedto` varchar(100) DEFAULT NULL,
  `user` varchar(100) DEFAULT NULL,
  `tabid` int(11) DEFAULT NULL,
  `finalstate` varchar(100) DEFAULT NULL,
  PRIMARY KEY (`entitylogid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8]]></sql>
</table>
