<tables>
<table>
<name>vtiger_processflow</name>
<sql><![CDATA[CREATE TABLE `vtiger_processflow` (
  `processflowid` int(11) default NULL,
  `processflowname` varchar(255) default NULL,
  `description` varchar(255) DEFAULT NULL,
  `starttasksubstatus` varchar(255) DEFAULT NULL,
  `processflowcase` varchar(255) DEFAULT NULL,
  `tasksla` varchar(255) DEFAULT NULL,
  `cuestinario` varchar(255) DEFAULT NULL,
  `processflowsecurity` varchar(255) DEFAULT NULL,
  `linktoprocesstemplate` varchar(100) DEFAULT NULL,
  `end_subst` varchar(50) DEFAULT NULL,
  `livello` varchar(10) DEFAULT NULL,
  `stato` varchar(50) DEFAULT NULL,
  `defaultvl` varchar(3) DEFAULT NULL,
  `attivo` varchar(3) DEFAULT NULL,
  `starttaskname` varchar(60) DEFAULT NULL,
  `endtaskname` varchar(60) DEFAULT NULL,
  `todoaction` varchar(200) DEFAULT NULL,
  `sequencer` varchar(250) DEFAULT NULL,
  `alertsettingmodel` varchar(5) DEFAULT '',
  `mailer_action` varchar(100) DEFAULT NULL,
  `manual` varchar(100) DEFAULT NULL,
  `movestocksequencer` varchar(100) DEFAULT NULL,
  `flussoparalello` varchar(200) DEFAULT NULL,
  PRIMARY KEY (`processflowid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8]]></sql>
</table>
<table>
<name>vtiger_processflowcf</name>
<sql><![CDATA[CREATE TABLE `vtiger_processflowcf` (
  `processflowid` int(11) NOT NULL,
  PRIMARY KEY (`processflowid`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8]]></sql>
</table>
